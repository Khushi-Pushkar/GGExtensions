{
    "manifest_version": 3,
    "name": "Chopsticks",
    "description": "Chopsticks Game",
    "version": "1.0",
    "permissions": [
        "storage",
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "action": {
        "default_popup": "index.html"
    }
}